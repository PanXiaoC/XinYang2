<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="org.nsig.ocp.Dao.checkchac_9" >
    <delete id="deletopacinf"  parameterType="org.nsig.ocp.Entity.opac_inf">
        update de_flag = '1',
                del_time = NOW()
            from opac_inf where inf_usern = #{inf_usern}
